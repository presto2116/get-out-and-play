<div class='activity-list'>
	<% @activities.reverse_each do |activity| %>
	<h1><%= activity.title %></h1>
	<p>Creator: <%= activity.creator %></p>
	<p>Date: <%= activity.date %> </p>
	<p><%= link_to "Show Details", activity_path(activity)%></p>
	<% end %>
	<p><%= link_to "New Activity", new_activity_path %>
	<p><%= link_to 'Signout', destroy_user_session_path, :method => :delete %></p>
	<p><%= link_to 'Sign Up', new_user_registration_path %></p>
</div>